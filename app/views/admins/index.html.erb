<h2>Users</h2>
<table class="table table-striped">
  <tr>
    <td>Id</td>
    <td>Venmo ID</td>
    <td>Access Token</td>
    <td>Refresh Token</td>
  </tr>
  <% @users.each do |u| %>
    <tr>
      <td><%= u.id %></td>
      <td><%= u.venmo_id %></td>
      <td><%= u.access_token %></td>
      <td><%= u.refresh_token %></td>
    </tr>
  <% end %>
</table>
<h2>Groups</h2>
<table class="table table-striped">
  <tr>
    <td>Id</td>
    <td>Name</td>
    <td>Members</td>
  </tr>
  <% @groups.each do |g| %>
    <tr>
      <td><%= g.id %></td>
      <td><%= g.name %></td>
      <td><%= g.members %></td>
    </tr>
  <% end %>
</table>
<h2>Transactions</h2>
<table class="table table-striped">
  <tr>
    <td>Id</td>
    <td>Group</td>
    <td>User</td>
    <td>Amount</td>
    <td>Note</td>
    <td>Action</td>
    <td>Venmo Transaction IDs</td>
  </tr>
  <% @transactions.each do |t| %>
    <tr>
      <td><%= t.id %></td>
      <td><%= t.group %></td>
      <td><%= t.user %></td>
      <td><%= t.amount %></td>
      <td><%= t.note %></td>
      <td><%= t.action %></td>
      <td><%= t.venmo_transaction_ids %></td>
    </tr>
  <% end %>
</table>